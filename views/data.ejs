<!-- Patients Table -->
<h2>Patients</h2>
<table>
  <thead>
    <tr>
      <th>Patient ID</th>
      <th>First Name</th>
      <th>Last Name</th>
      <th>Date of Birth</th>
    </tr>
  </thead>
  <tbody>
    <% results.forEach(row => { %>
      <tr>
        <td><%= row.patient_id %></td>
        <td><%= row.first_name %></td>
        <td><%= row.last_name %></td>
        <td><%= row.date_of_birth %></td>
      </tr>
    <% }); %>
  </tbody>
</table>

